<?xml version="1.0" encoding="utf-8"?>
<merge android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.Tabs.Home.HomeViewLayout"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/homeViewRefresher">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <com.gethighlow.highlowandroid.CustomViews.Other.HighLowView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/highlowview"
                    android:paddingBottom="100dp"
                    />
            </ScrollView>

    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
</merge>